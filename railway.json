{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
      "nixpacksPlan": {
        "phases": {
          "install": {
            "dependsOn": ["setup"],
            "cmds": ["pip install poetry==$NIXPACKS_POETRY_VERSION && poetry install --no-interaction --no-ansi"]
          }
        }
      }
    }
  }